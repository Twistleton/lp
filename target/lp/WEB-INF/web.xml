<?xml version="1.0" encoding="UTF-8"?>
<web-app
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
  http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
        version="2.5">
    <display-name>my Web App</display-name>
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>CutterbuchungController</servlet-name>
        <servlet-class>com.rolfbenz.lp.CutterbuchungController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CutterbuchungController</servlet-name>
        <url-pattern>/Cutterbuchung.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>TeamverwaltungController</servlet-name>
        <servlet-class>com.rolfbenz.lp.TeamverwaltungController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TeamverwaltungController</servlet-name>
        <url-pattern>/Teamverwaltung.do</url-pattern>
    </servlet-mapping>
</web-app>
